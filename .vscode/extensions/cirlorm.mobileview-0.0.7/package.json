{
	"name": "mobileview",
	"displayName": "MobileView",
	"description": "Provides a responsive mobile view for testing mobile view of your apps and websites directly form vs code.",
	"publisher": "cirlorm",
	"version": "0.0.7",
	"icon": "media/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/champ3oy/MobileView"
	},
